{
  "version": 2,
  "builds": [
    {
      "src": "server-fastify.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server-fastify.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
